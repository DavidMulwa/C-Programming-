//Function to find volume of a sphere
#include<stdio.h>
#define pi 3.142
int volume(int r);
int main()
{
    int a,volume;
    printf("enter value of a:");
    scanf("%d",&a);
    volume=4/3*pi*a*a*a;
    printf("the volume is %d",volume);
    return 0;
}
//Function definition
int volume(int a)
{
    int volume;
    volume=4/3*pi*a*a*a;
    return volume;
}
